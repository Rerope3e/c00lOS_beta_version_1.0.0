-- Scripts:

local function ELFCU_fake_script() -- Sky.LocalScript 
	local script = Instance.new('LocalScript', Sky)

	local button = script.Parent
	local Lighting = game:GetService("Lighting")
	local decalId = "rbxassetid://8408806737"
	
	button.MouseButton1Click:Connect(function()
		print("Skybox button clicked")
	
		for _, obj in ipairs(Lighting:GetChildren()) do
			if obj:IsA("Sky") then
				obj:Destroy()
			end
		end
	
		local sky = Instance.new("Sky")
		sky.Name = "CustomSkybox"
		sky.SkyboxBk = decalId
		sky.SkyboxDn = decalId
		sky.SkyboxFt = decalId
		sky.SkyboxLf = decalId
		sky.SkyboxRt = decalId
		sky.SkyboxUp = decalId
	
		sky.SunTextureId = ""
		sky.MoonTextureId = ""
	
		sky.Parent = Lighting
	
	end)
	
end
coroutine.wrap(ELFCU_fake_script)()
local function EHXFP_fake_script() -- Decal.LocalScript 
	local script = Instance.new('LocalScript', Decal)

	local button = script.Parent  
	local decalId = "rbxassetid://8408806737"
	
	local faces = {
		Enum.NormalId.Top,
		Enum.NormalId.Bottom,
		Enum.NormalId.Front,
		Enum.NormalId.Back,
		Enum.NormalId.Left,
		Enum.NormalId.Right
	}
	
	button.MouseButton1Click:Connect(function()
	
		for _, obj in pairs(workspace:GetDescendants()) do
			if obj:IsA("BasePart") then
				for _, face in pairs(faces) do
					local alreadyExists = false
					for _, child in pairs(obj:GetChildren()) do
						if child:IsA("Decal") and child.Face == face and child.Texture == decalId then
							alreadyExists = true
							break
						end
					end
	
					if not alreadyExists then
						local decal = Instance.new("Decal")
						decal.Texture = decalId
						decal.Face = face
						decal.Parent = obj
					end
				end
			end
		end
	end)
	
end
coroutine.wrap(EHXFP_fake_script)()
local function TAIR_fake_script() -- Destroy.LocalScript 
	local script = Instance.new('LocalScript', Destroy)

	local guiName = "DestroySubjectGUI"
	local player = game.Players.LocalPlayer
	local pg = player and player:WaitForChild("PlayerGui")
	local gui = pg and pg:FindFirstChild(guiName)
	if gui then gui:Destroy() end
	
end
coroutine.wrap(TAIR_fake_script)()
