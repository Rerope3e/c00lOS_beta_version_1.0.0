Sky.Font = Enum.Font.SourceSans
Sky.Text = "Sky"
Sky.TextColor3 = Color3.fromRGB(255, 255, 255)
Sky.TextScaled = true
Sky.TextSize = 14.000
Sky.TextWrapped = true

Decal.Name = "Decal"
Decal.Parent = DestroySubjectGUI
Decal.BackgroundColor3 = Color3.fromRGB(0, 124, 8)
Decal.BorderColor3 = Color3.fromRGB(0, 0, 0)
Decal.BorderSizePixel = 0
